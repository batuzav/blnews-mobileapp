{
  "expo": {
    "name": "MyBL-News",
    "slug": "my-blnews",
    "extra": {
      "serverUrl": "http://mybl-news-be.herokuapp.com"
    },
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./images/launchimages/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.bodylogic.myblnews",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.bodylogic.myblnews",
      "versionCode": 1,
      "permissions": ["NOTIFICATIONS", "USER_FACING_NOTIFICATIONS", "VIBRATE"],
      "googleServicesFile": "./google-services.json"
    },
    "description": ""
  }
}
